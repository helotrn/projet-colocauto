{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "repositories": [
        {
            "type": "git",
            "url":  "https://gitlab.com/molotovcoop/laravel-api-molotov.git"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "ext-pdo_pgsql": "^7.2",
        "doctrine/dbal": "^2.10",
        "eluceo/ical": "^0.16.0",
        "fideloper/proxy": "^4.0",
        "intervention/image": "^2.5",
        "johngrogg/ics-parser": "^2.1",
        "laravel/framework": "^6.0",
        "laravel/helpers": "^1.1",
        "laravel/passport": "^7.0",
        "maatwebsite/excel": "^3.1",
        "molotovcoop/laravel-api-molotov": "dev-master",
        "phaza/laravel-postgis": "^4.0",
        "predis/predis": "^1.1",
        "sentry/sentry-laravel": "^1.7",
        "stripe/stripe-php": "^7.27",
        "symfony/expression-language": "^4",
        "vkovic/laravel-custom-casts": "^1.2"
    },
    "require-dev": {
        "beyondcode/laravel-dump-server": "^1.0",
        "deployer/deployer": "^6.6",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "laravel/tinker": "^2.2",
        "limedeck/phpunit-detailed-printer": "^4.0",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^7.5",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "files": [
            "app/helpers.php"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
